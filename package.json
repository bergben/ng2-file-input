{
    "name": "ng2-file-input",
    "version": "1.0.15",
    "description": "Angular component that implements a drag and drop or select file selection, including preview",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bergben/ng2-file-input.git"
    },
    "license": "MIT",
    "module": "./dist/ng2-file-input.js",
    "main": "./dist/bundles/ng2-file-input.umd.js",
    "typings": "./dist/ng2-file-input.d.ts",
    "scripts": {
        "transpile": "ngc",
        "package": "rollup -c",
        "minify": "uglifyjs dist/bundles/ng2-file-input.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/ng2-file-input.umd.min.js",
        "build": "npm run transpile && npm run package && npm run minify"
    },
    "author": {
        "name": "Benedikt Berger",
        "email": "benedikt.berger@live.de"
    },
    "keywords": [
        "angular",
        "angular2",
        "angular4",
        "file",
        "input",
        "drag and drop",
        "preview"
    ],
    "dependencies": {
        "bergben-angular2-file-drop": ">=0.2.0"
    },
    "peerDependencies": {
        "@angular/core": ">=4.0.0",
        "@angular/platform-browser": ">=4.0.0",
        "@angular/common": ">=4.0.0"
    },
    "devDependencies": {
        "@angular/common": ">=5.0.0",
        "@angular/compiler": ">=5.0.0",
        "@angular/compiler-cli": ">=5.0.0",
        "@angular/core": ">=5.0.0",
        "@angular/platform-browser": ">=5.0.0",
        "@angular/platform-browser-dynamic": ">=5.0.0",
        "@angular/platform-server": ">=5.0.0",
        "@angular/router": ">=5.0.0",
        "@types/node": "^8.0.50",
        "core-js": ">=2.4.1",
        "reflect-metadata": ">=0.1.3",
        "rollup": "^0.51.2",
        "rxjs": "5.1.1",
        "tslint": "^5.8.0",
        "typescript": "~2.4.2",
        "uglify-js": "^3.1.8",
        "zone.js": "^0.8.18"
    },
    "bugs": {
        "url": "https://github.com/bergben/ng2-file-input/issues"
    },
    "homepage": "https://github.com/bergben/ng2-file-input#readme"
}